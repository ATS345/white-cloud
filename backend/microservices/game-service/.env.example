# 游戏服务 - 环境变量配置示例

# 服务器配置
PORT=3002
NODE_ENV=development

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=game_store
DB_USER=root
DB_PASSWORD=password
DB_DIALECT=mysql

# Redis 配置
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL=3600

# 日志配置
LOG_LEVEL=info
LOG_FILE_PATH=./logs/application.log

# 服务注册配置
SERVICE_NAME=game-service
SERVICE_ID=game-service-1
SERVICE_TAGS=game,microservice
SERVICE_HOST=localhost
SERVICE_PORT=3002
